{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "declaration": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "noLib": false,
    "suppressImplicitAnyIndexErrors": true,
    "baseUrl": "src",
    "paths": {
      "containers": ["./containers"]
    }
  },
  "compileOnSave": false,
  "exclude": [
    "node_modules"
  ],
  "include": ["src"]
}